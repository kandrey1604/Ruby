@model RubyAdmin.Models.StructureOfUserProperty
@{
    ViewBag.Title = "Удаление состава недвижимостей";
}

<div>
    <dl class="dl-horizontal">
        <dt>Код недвижимости</dt>
        <dd>
            @Html.DisplayFor(model => model.PropertyId)
        </dd>
        <dt>Код пользователя</dt>
        <dd>
            @Html.DisplayFor(model => model.UserId)
        </dd>
    </dl>

    <div>
        <form asp-controller="Home" asp-action="DeleteStructureOfUserProperty" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>
