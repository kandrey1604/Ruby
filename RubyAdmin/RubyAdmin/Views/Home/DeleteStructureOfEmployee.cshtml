@model RubyAdmin.Models.StructureOfEmployee
@{
    ViewBag.Title = "Удаление состава сотрудников";
}

<div>
    <dl class="dl-horizontal">
        <dt>Код недвижимости</dt>
        <dd>
            @Html.DisplayFor(model => model.PropertyId)
        </dd>
        <dt>Код сотрудника</dt>
        <dd>
            @Html.DisplayFor(model => model.EmployeeId)
        </dd>
        <dt>Код должности</dt>
        <dd>
            @Html.DisplayFor(model => model.CurrentPositionId)
        </dd>
    </dl>

    <div>
        <form asp-controller="Home" asp-action="DeleteStructureOfEmployee" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>
